{
  "name": "coding-quiz",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "cd server && npm run start",
    "seed": "cd server && npm run seed",
    "develop": "concurrently \"cd server && npm run watch\" \"cd client && npm run dev\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "build": "concurrently \"cd server && npm run build\" \"cd client && npm run build\"",
    "render-build": "npm install && npm run build",
    "test": "npx cypress run --browser electron",
    "cypress": "npx cypress open",
    "test:component": "npx cypress run --component",
    "test:e2e": "npx cypress run",
    "test:gui": "npx cypress open --detached",
    "coverage": "npx nyc report",
    "test:coverage": "npm run test:component && npm run test:e2e && npm run coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@cypress/code-coverage": "^3.12.44",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@cypress/react": "^9.0.0",
    "@testing-library/cypress": "^10.0.2",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.1",
    "@types/node": "^22.13.10",
    "concurrently": "^8.2.0",
    "cypress": "^13.13.0",
    "react": "^18.3.1",
    "vite-plugin-istanbul": "^6.0.2",
    "react-dom": "^18.3.1",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^4.3.1",
    "dotenv": "^16.4.7",
    "mongoose": "^8.12.1",
    "vitest": "^3.0.9"
  }
}
